{
  "name": "Ruby (IRB)",
  "description": "Ruby IRB interactive shell built with Native Client",
  "version": "2.0.0.%(version)s",
  "manifest_version": 2,
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "storage",
    { "socket": [
        "tcp-listen:*:*",
        "tcp-connect",
        "resolve-host",
        "udp-bind:*:*",
        "udp-send-to:*:*"
      ]
    }
  ]
}
